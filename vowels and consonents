#include <stdio.h>
#include<stdlib.h>

int main()
{
    char d,l_c,u_c;
    printf("Enter the charecter");
    scanf("%c",&d);
    l_c='a'||'e'||'i'||'o'||'u';
    u_c='A'||'E'||'I'||'O'||'U';
    if(l_c||u_c)
    {
        printf("%c is a vowel",d);
    }
    else{
        printf("%c is a consonent",d);
    }
    return 0;
}
